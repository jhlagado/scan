07E2   D6 30                  SUB   $30   ;       Form decimal digit
07E4   85                     ADD   A,L   ;       Add into bottom of HL
07E5   6F                     LD   L,A   ; 
07E6   3E 00                  LD   A,00   ;       Clear A
07E8   8C                     ADC   A,H   ; Add with carry H-reg
07E9   67                     LD   H,A   ; Put result in H-reg
07EA   03                     INC   BC   ;       Increment IP
07EB   0A                     LD   A,(BC)   ;       and get the next character
07EC   FE 30                  CP   $30   ;       Less than $30
07EE   38 0C                  JR   C,num2   ;       Not a number / end of number
07F0   FE 3A                  CP   $3A   ;       Greater or equal to $3A
07F2   30 08                  JR   NC,num2   ;       Not a number / end of number
07F4                             ; Multiply digit(s) in HL by 10
07F4   29                     ADD   HL,HL   ;        2X
07F5   5D                     LD   E,L   ;        LD DE,HL
07F6   54                     LD   D,H   ; 
07F7   29                     ADD   HL,HL   ;        4X
07F8   29                     ADD   HL,HL   ;        8X
07F9   19                     ADD   HL,DE   ;        2X  + 8X  = 10X
07FA                             ; 52t cycles
07FA   18 E6                  JR   num1   
07FC                NUM2:        
07FC   0B                     DEC   BC   
07FD   E5                     PUSH   HL   ; Put the number on the stack
07FE   FD E9                  JP   (IY)   ; and process the next character
0800                HEX:         ;=26
0800   21 00 00               LD   HL,0   ; Clear HL to accept the number
0803                HEX1:        
0803   03                     INC   BC   
0804   0A                     LD   A,(BC)   ; Get the character which is a numeral
0805   CB 77                  BIT   6,A   ; is it uppercase alpha?
0807   28 02                  JR   Z,hex2   ; no a decimal
